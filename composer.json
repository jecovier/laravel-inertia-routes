{
    "name": "jecovier/inertia-route",
    "description": "use inertia components as pages",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": ">=8.0",
        "inertiajs/inertia-laravel": "^0.3.3"
    },
    "license": "MIT",
    "keywords": [
      "jecovier",
      "inertia",
      "routes",
      "laravel",
      "automatic",
      "svelte",
      "react",
      "vue",
      "php"
    ],
    "authors": [
        {
            "name": "Jose Javier Espinoza",
            "email": "jecovier@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "Jecovier\\InertiaRoute\\": "src"
      }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "InertiaRoute": "Jecovier\\InertiaRoute\\InertiaRoute"
            }
        }
    }
}